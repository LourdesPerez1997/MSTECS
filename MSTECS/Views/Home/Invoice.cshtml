@model List<MSTECS.Models.InvoiceDetail>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
</div>

<div class="card">
    <div class="card-header">
        <h5>Client List</h5>
    </div>
    <div class="card-body">
        <a class="btn btn-success btn-sm" asp-action="Client_Detail" asp-controller="Home" asp-route-idClient="0">New Invoice</a>
        <hr />

        <table class="table tabletable-striped">
            <thead>
                <tr>
                    <th>Client Name</th>
                    <th>Product</th>
                    <th>Price</th>
                    <th>Quantity</th>
                    <th>Amount</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                
            </tbody>
        </table>
    </div>
</div>